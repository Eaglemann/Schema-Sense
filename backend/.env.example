# Backend Configuration
# Copy this file to .env and update the values as needed

# API Keys (Required for AI descriptions)
GROQ_API_KEY=your_groq_api_key_here

# Server Configuration  
HOST=0.0.0.0
PORT=8000
DEBUG=false

# CORS Configuration (for separate frontend)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,https://your-frontend-app.netlify.app

# File Processing Limits
MAX_FILE_SIZE=104857600  # 100MB in bytes
ANALYSIS_TIMEOUT=300     # 5 minutes in seconds

# Application Info
APP_NAME=Schema Generator
APP_VERSION=2.0